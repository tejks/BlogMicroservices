name: dockercompose7989031477573040332
services:
  commentsapi:
    build:
      context: C:\Users\szymo\source\repos\BlogMicroservices
      dockerfile: CommentsAPI/Dockerfile
    depends_on:
      mongo_db:
        condition: service_started
    environment:
      MONGO_URI: mongodb://mongo_db:27017
    image: commentsapi
    networks:
      default: null
  mongo_db:
    container_name: db_container
    image: mongo:latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 27017
      published: "27018"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: mongo_db
      target: /data/db
      volume: {}
  postsapi:
    build:
      context: C:\Users\szymo\source\repos\BlogMicroservices
      dockerfile: PostsAPI/Dockerfile
    depends_on:
      mongo_db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      MONGO_URI: mongodb://mongo_db:27017
    image: postsapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\szymo\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\szymo\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose7989031477573040332_default
volumes:
  mongo_db:
    name: dockercompose7989031477573040332_mongo_db